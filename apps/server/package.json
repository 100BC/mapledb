{
  "name": "@mooseical/server",
  "version": "0.0.0",
  "main": "src/index.ts",
  "private": true,
  "engines": {
    "node": ">=16",
    "pnpm": ">=6"
  },
  "scripts": {
    "dev": "nodemon --watch src/ -r dotenv/config -r tsconfig-paths/register -T src/index.ts",
    "build": "tsc --build --clean && tsc && tsc-alias",
    "start": "node -r dotenv/config dist/index.js",
    "generate": "prisma generate",
    "postinstall": "prisma generate",
    "db": "prisma studio",
    "db:push": "prisma db push && prisma format",
    "db:migrate": "prisma migrate dev && prisma format",
    "db:reset": "prisma migrate reset",
    "lint": "eslint --fix 'src/**/*.ts'",
    "clean": "rm -rf node_modules .turbo .pnpm-debug.log dist"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  },
  "dependencies": {
    "@pothos/core": "^3.3.2",
    "@pothos/plugin-complexity": "^3.1.2",
    "@pothos/plugin-scope-auth": "^3.4.1",
    "@pothos/plugin-validation": "^3.0.2",
    "@prisma/client": "^3.10.0",
    "altair-fastify-plugin": "^4.4.0",
    "cloudinary": "^1.28.1",
    "dotenv": "^16.0.0",
    "fastify": "^3.27.2",
    "fastify-compress": "^4.0.1",
    "fastify-cors": "^6.0.3",
    "fastify-helmet": "^7.0.1",
    "fastify-rate-limit": "^5.7.2",
    "graphql": "^16.3.0",
    "graphql-scalars": "^1.14.1",
    "graphql-upload": "^13.0.0",
    "mercurius": "^9.3.3",
    "mercurius-upload": "^3.0.1",
    "pino-pretty": "^7.5.1",
    "zod": "^3.12.0"
  },
  "devDependencies": {
    "@mooseical/eslint-config": "workspace:*",
    "@mooseical/tsconfig": "workspace:*",
    "@types/faker": "5.5.3",
    "@types/graphql-upload": "^8.0.11",
    "@types/node": "^16.11.26",
    "@typescript-eslint/eslint-plugin": "^5.12.1",
    "@typescript-eslint/parser": "^5.12.1",
    "eslint": "^8.10.0",
    "faker": "5.5.3",
    "kill-port": "^1.6.1",
    "nodemon": "^2.0.15",
    "prisma": "^3.10.0",
    "ts-node": "^10.5.0",
    "tsc-alias": "^1.6.3",
    "tsconfig-paths": "^3.12.0",
    "typescript": "^4.5.5"
  }
}
