{
  "name": "@mooseical/server",
  "version": "0.0.0",
  "main": "src/index.ts",
  "private": true,
  "engines": {
    "node": ">=16",
    "pnpm": ">=6"
  },
  "scripts": {
    "start": "node -r dotenv/config dist/index.js",
    "dev": "nodemon --watch src/ -r dotenv/config -r tsconfig-paths/register -T src/index.ts",
    "build": "tsc --build --clean && tsc && tsc-alias",
    "generate": "prisma generate",
    "db": "prisma studio",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:reset": "prisma migrate reset",
    "postmigrate": "prisma format && prisma generate",
    "postinstall": "prisma generate",
    "lint:code": "eslint src --ext .js,.jsx,.ts",
    "format:prisma": "prisma format",
    "format:code": "eslint src --ext .js,.ts --fix && prettier --write .",
    "format:all": "eslint src --ext .js,.ts --fix && prisma format && prettier --write .",
    "clean": "rm -rf node_modules .turbo .pnpm-debug.log dist"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  },
  "dependencies": {
    "@pothos/core": "^3.0.0",
    "@pothos/plugin-complexity": "^3.0.0",
    "@pothos/plugin-scope-auth": "^3.0.0",
    "@pothos/plugin-validation": "^3.0.0",
    "@prisma/client": "^3.0.0",
    "altair-fastify-plugin": "^4.1.0",
    "cloudinary": "^1.28.1",
    "dotenv": "^16.0.0",
    "fastify": "^3.23.1",
    "fastify-compress": "^4.0.0",
    "fastify-cors": "^6.0.2",
    "fastify-helmet": "^7.0.0",
    "fastify-rate-limit": "^5.6.2",
    "graphql": "^16.2.0",
    "graphql-scalars": "^1.9.0",
    "graphql-upload": "^13.0.0",
    "mercurius": "^9.0.0",
    "mercurius-upload": "^3.0.0",
    "pino-pretty": "^7.5.1",
    "zod": "^3.11.6"
  },
  "devDependencies": {
    "@mooseical/eslint-config": "workspace:*",
    "@mooseical/tsconfig": "workspace:*",
    "@types/faker": "^5.5.4",
    "@types/graphql-upload": "^8.0.7",
    "@types/node": "^17.0.0",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.2.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^16.0.0",
    "eslint-config-prettier": "^8.2.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-security": "^1.4.0",
    "faker": "^5.5.3",
    "kill-port": "^1.6.1",
    "nodemon": "^2.0.7",
    "prisma": "^3.0.0",
    "ts-node": "^10.0.0",
    "tsc-alias": "^1.4.1",
    "tsconfig-paths": "^3.10.1",
    "typescript": "^4.5.5"
  }
}
