{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "generate": {
      "outputs": []
    },
    "build": {
      "dependsOn": ["generate", "^build"],
      "outputs": ["dist/**", ".next/**"]
    },
    "deploy": {
      "outputs": [],
      "dependsOn": ["build", "lint"]
    },
    "start": {},
    "lint": {
      "outputs": [],
      "dependsOn": ["generate"]
    },
    "dev": {
      "cache": false
    },
    "clean": {
      "cache": false
    }
  }
}
